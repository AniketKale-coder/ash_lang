{
  "name": "ashlang",
  "displayName": "AshLang",
  "description": "AshLang Language support for VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ashlang",
      "aliases": ["AshLang", "ashlang"],
      "extensions": [".ash"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ashlang",
      "scopeName": "source.ash",
      "path": "./syntaxes/ashlang.tmLanguage.json"
    }]
  }
}
